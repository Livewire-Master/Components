<?php

namespace [namespace];

use Illuminate\Contracts\View\View;
use Livewire\Component;

class [class] extends Component
{
    /**
     * Front-End Data bag
     *
     * @return array
     */
    public function bag(): array
    {
        return [];
    }

    /**
     * Render the component view
     *
     * @return View
     */
    public function render(): View
    {
        return view('[view]', $this->bag());
    }
}
